    "@angular/router": "^14.2.0",
    "@ionic/angular": "^6.1.10",
    "@ionic/core": "^6.1.10",
    "ionicons": "^6.0.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.35",
