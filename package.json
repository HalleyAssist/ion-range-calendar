    "@angular/platform-browser-dynamic": "^15.0.1",
    "@angular/router": "^15.0.1",
    "@ionic/angular": "^6.3.8",
    "@ionic/core": "^6.3.8",
    "ionicons": "^6.0.4",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.39",
